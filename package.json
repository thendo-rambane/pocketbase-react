{
  "name": "pocketbase-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pocketbase": "cd pocketbase && pocketbase serve",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "dependencies": {
    "pocketbase": "^0.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/pocketbase-react.es.js",
      "require": "./dist/pocketbase-react.umd.js"
    }
  },
  "main": "./dist/pocketbase-react.umd.js",
  "module": "./dist/pocketbase-react.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@vitejs/plugin-react": "^2.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.0",
    "react-hooks": "^1.0.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "pocketbase": "^0.8.2",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thendo-rambane/pocketbase-react"
  },
  "author": "Thendo Rambane",
  "license": "MIT"
}
